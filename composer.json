{
  "name": "martin-ro/filament-blocks",
  "require": {
    "php": "^8.3",
    "filament/filament": "^3.2",
    "illuminate/contracts": "^10.0 | ^11.0",
    "spatie/laravel-package-tools": "^1.16.4"
  },
  "autoload": {
    "psr-4": {
      "MartinRo\\FilamentBlocks\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "MartinRo\\FilamentBlocks\\FilamentBlocksServiceProvider"
      ],
      "aliases": {
        "FilamentBlocks": "MartinRo\\FilamentBlocks\\Facades\\FilamentBlocks"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
